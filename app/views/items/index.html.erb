<h1>All Items</h1>
<% @items.each do |item| %>
  <div class="media">
    <div class="media-body">
      <div class="media-heading">
        <%= link_to item.title, item %>
        <p>Owned by <%= item.user.name %></p>
        <p>Time Remaining <%= item.remaining_time %></p>
      </div>
    </div>
  </div>
<% end %>

